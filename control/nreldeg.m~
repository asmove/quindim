function nreldeg(f, G, hi, x)
    i = 0;
    is_equal_zero = true;
    lie_f_h = hi;
    
    [~, m] = size(G);
    
    while(is_equal_zero)
        lie_G = [];
        for i = 1:m
            lid_gi = lie_diff(lie_f_h, G(i), x); 
            if(~isempty(symvar(lid_gi)))
                is_equal_zero = false;
                break;
            end
            
            lie_G = [lie_G, lie_gi];
        end
        
        if(~is_equal_zero)
           brak 
        end
        
        i = i + 1;
    end
end